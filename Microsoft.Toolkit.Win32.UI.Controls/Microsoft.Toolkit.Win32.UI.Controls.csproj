<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net47</TargetFramework>
    <RootNamespace>Microsoft.Toolkit.Win32.UI.Controls</RootNamespace>
    <AssemblyName>Microsoft.Toolkit.Win32.UI.Controls</AssemblyName>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <RunCodeAnalysis>true</RunCodeAnalysis>

    <Title>UWP Community Toolkit Win32 Controls</Title>
    <Description>This library provides XAML user controls for Win32. It is part of the UWP Community Toolkit.</Description>
    <PackageTags>WebView Win32 WPF WindowsForms</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="PresentationCore" />
    <Reference Include="PresentationFramework" />
    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="System.Design" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xaml" />
    <Reference Include="Windows">
      <!-- Need Metadata contract 6.0 or later -->
      <!-- 17134: Insider Preview (Fast), Spring Creators (RS4) -->
      <HintPath Condition="Exists('$(MSBuildProgramFiles32)\Windows Kits\10\UnionMetadata\10.0.17134.0\Windows.winmd')">$(MSBuildProgramFiles32)\Windows Kits\10\UnionMetadata\10.0.17134.0\Windows.winmd</HintPath>
      <!-- 17643: Insider Skip Ahead -->
      <HintPath Condition="Exists('$(MSBuildProgramFiles32)\Windows Kits\10\UnionMetadata\10.0.17643.0\Windows.winmd')">$(MSBuildProgramFiles32)\Windows Kits\10\UnionMetadata\10.0.17643.0\Windows.winmd</HintPath>
      <Private>False</Private>
    </Reference>
    <Reference Include="WindowsBase" />
    <PackageReference Include="System.Runtime.WindowsRuntime" PrivateAssets="All">
      <Version>4.0.0</Version>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <Compile Update="DesignerUI.Designer.cs">
      <DependentUpon>DesignerUI.resx</DependentUpon>
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
    </Compile>
    <Compile Update="WinForms\WebView\WebView.BaseProperties.cs">
      <SubType>Component</SubType>
      <DependentUpon>WebView.cs</DependentUpon>
    </Compile>
    <Compile Update="WinForms\WebView\WebView.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="WinForms\WebView\WebView.Designer.cs">
      <DependentUpon>WebView.cs</DependentUpon>
    </Compile>
    <Compile Update="WinForms\WebView\WebView.Events.cs">
      <SubType>Component</SubType>
      <DependentUpon>WebView.cs</DependentUpon>
    </Compile>
    <Compile Update="WinForms\WebView\WebView.Init.cs">
      <SubType>Component</SubType>
      <DependentUpon>WebView.cs</DependentUpon>
    </Compile>
    <Compile Update="WinForms\WebView\WebView.Navigation.cs">
      <SubType>Component</SubType>
      <DependentUpon>WebView.cs</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="DesignerUI.resx">
      <LastGenOutput>DesignerUI.Designer.cs</LastGenOutput>
      <Generator>ResXFileCodeGenerator</Generator>
    </EmbeddedResource>
  </ItemGroup>

</Project>
